<h1>Voting Results</h1>
<%= for item <- @responses do %>
    <h2>Ballot Item</h2>
    <table>
        <thead>
            <th>User</th>
            <th>Response</th>
        </thead>
        <tbody>
            <%= for response <- item do %>
                <tr>
                    <td><%= response.account_id %></td>
                    <td><%= inspect(response.response) %></td>
                </tr>
            <% end %>
        </tbody>
    </table>
<% end %>


